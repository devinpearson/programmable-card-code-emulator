{
  "name": "programmable-card-code-emulator",
  "version": "1.1.0",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "npm run clean && tsc -p ./tsconfig.json && npm run formatjs",
    "clean": "rimraf src",
    "format": "npm run prettier -- --write",
    "formatjs": "npm run prettierjs -- --write",
    "lint": "eslint ts_src/** src/**/*.js",
    "prettier": "prettier \"ts_src/**/*.ts\" \"test/**/*.ts\" --ignore-path ./.prettierignore",
    "prettierjs": "prettier \"src/**/*.js\" --ignore-path ./.prettierignore"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "investec",
    "card",
    "emulator",
    "programmable banking",
    "open banking",
    "banking"
  ],
  "author": "Devin Pearson",
  "license": "MIT",
  "description": "An emulator for the Programmable Card Code Lamda function",
  "dependencies": {
    "lodash": "^4.17.21",
    "moment-mini": "^2.29.4",
    "node-fetch": "^2.7.0",
    "uuid": "^9.0.1",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@types/node": "^20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "validator": "^13.11.0"
  }
}
